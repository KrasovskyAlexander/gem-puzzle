{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules":{
        "no-plusplus": 0,
        "eqeqeq": 0,
        "no-shadow": 0,
        "no-continue": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "func-names": 0,
        "linebreak-style": 0
    } 
}